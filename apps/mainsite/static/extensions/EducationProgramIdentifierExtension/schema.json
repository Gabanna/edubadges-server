{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Education Program Identifier Extension",
  "description": "An extension that allows you to add multiple number EducationProgramIdentifier to a badgeclass object.",
  "type": "object",
  "properties": {
    "EducationProgramIdentifier": {
      "anyOf": [
        {
          "type": "array",
          "items": {
            "type": "number"
          }
        },
        {
          "type": "number"
        }
      ]
    }
  },
  "required": [
    "EducationProgramIdentifier"
  ]
}